<?xml version="1.0" ?>
<odoo>
        <record id="module_irastorage_category" model="ir.module.category">
            <field name="name">Irastorage</field>
            <field name="sequence">17</field>
        </record>
        <record id="irastorage_group_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_irastorage_category"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="irastorage_group_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="implied_ids" eval="[(4, ref('irastorage_group_user'))]"/>
            <field name="category_id" ref="module_irastorage_category"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
</odoo>

